#User function Template for python3

#User function Template for python3
class Solution:
    def armstrongNumber (self, n):
        # code here 
        number  = str(n)
        sum = 0
        for i in number:
            sum+=(int(i)**3)
        if n == sum:
            return "Yes"
        else:
            return "No"


